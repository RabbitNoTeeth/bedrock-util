        </pluginRepository>
    </pluginRepositories>

    <properties>
        <java.version>21</java.version>
        <quartz.version>2.3.2</quartz.version>
        <yitter.version>1.0.6</yitter.version>
        <commons.io.version>2.14.0</commons.io.version>
        <commons.text.version>1.11.0</commons.text.version>
